class rectangle:
    area=0
    p=0
    def __init__(self,l,b):
        self.rl=l
        self.rb=b
    def area1(self):
        rectangle.area=self.rl*self.rb
        return self.area
    def peri(self):
        rectangle.p=2*(self.rl+self.rb)
        return self.p
    
rectangle1=rectangle(10,10)
rectangle2=rectangle(20,20)

print(rectangle1.area1())
print(rectangle1.peri())

print(rectangle2.area1())
print(rectangle2.peri())
